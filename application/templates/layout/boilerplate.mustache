</head>

<body>
	<img id="bgimg" src="/media/img/background.jpg" />
	<div id="realbody">
		<div class="container">
			<header class="masthead">
				<h1>RedditGW</h1>
				<h2>Event Signup website</h2>
			</header>
			<nav id="top_nav">
				<ul>
					<a id="alien" href="/"><img src="/media/img/alien.png" /></a>
					{{#links}}
					<li><a {{#location}}href="{{location}}"{{/location}}>{{text}} {{#icon}}&nbsp;<img src="{{src}}" />{{/icon}}</a>
						{{#dropdown}}
						<ul>
							{{#dropdown_links}}
							<li><a href="{{location}}">{{text}}</a></li>
							{{/dropdown_links}}
						</ul>
						{{/dropdown}}
					</li>
					{{/links}}
				</ul>
			</nav>
{{{notices}}}
{{>content}}
		<div class="push"></div>
		</div> <!-- end container -->
	</div> <!-- end realbody -->

	<!-- JavaScript at the bottom for fast page loading -->
	[[assets_body]]
	<!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if offline -->
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="/public_html/media/js/libs/jquery-1.6.2.min.js"><\/script>')</script>
	<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.min.js"></script>
	<script src="/media/js/jquery.fullscreenr.js" type="text/javascript"></script>
	<script>
		$('input[type=date]').datepicker({
			changeYear : true,
			dateFormat : 'yy-mm-dd',
			yearRange  : '1950:+2',
		});
		
		var FullscreenrOptions = {  width: 1280, height: 1200, bgID: '#bgimg' };
		jQuery.fn.fullscreenr(FullscreenrOptions);
	</script>
	<!-- scripts concatenated and minified via ant build script-->
	<script defer src="/media/js/plugins.js"></script>
	<script defer src="/media/js/script.js"></script>
	<script defer src="/media/js/notices.js"></script>
	<!-- end scripts-->

	
