	<h4>Currently available builds</h4>
	<table>
		<thead>
			<tr>
				<td colspan="2">Actions</td>
				<td>Name</td>
				<td>PvX link</td>
			</tr>
		</thead>
		<tbody>
{{#build_list}}
			<tr>
				<td><a href="{{create_url}}"><img src="/media/img/icons/page_add.png" title="Create event using {{name}}" /></a></td>
				<td><a href="{{edit_url}}"><img src="/media/img/icons/page_edit.png" title="Edit {{name}}" /></a></td>
				<td>{{name}}</td>
				<td>{{#url}}<a href="{{url}}" target="_blank">Build link</a>{{/url}}{{^url}}No link available{{/url}}</td>
			</tr>
{{/build_list}}
		</tbody>
	</table>

<a href="{{build_add_link}}">Add new build</a>