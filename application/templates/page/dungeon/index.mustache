	<section id="dungeons">
		<h3>Dungeons</h3>
		<a class="button" href="{{dungeon_add_url}}">Add new dungeon</a>
		<ul>
{{#dungeon_list}}
			<li>
				<div>
					<h4>{{name}}</h4>
				</div>
				<a class="delete" href="{{remove_url}}" title="Delete {{name}}">Delete</a>
				<a class="edit" href="{{edit_url}}"  title="Edit {{name}}">Edit</a>
			</li>
{{/dungeon_list}}
		</ul>
	</section>