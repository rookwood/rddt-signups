					<li class="event_expanded">
{{#event}}
						<ul class="header">
							<li class="event_title">
								<div>
									<h4>{{dungeon}}</h4>
									<h5>Manager: {{hostas}}</h5>
								</div>
							</li>
							<li class="event_numbers">
								<div>
									<h4><span class="player_count_low">4/8</span></h4>
									<h5>Players</h5>
								</div>
							</li>
							<li class="event_time">
								<div>
									<h4><time class="date" datetime="{{time_full}}">{{date}}</time></h4>
									<h5><time class="time" datetime="{{time_full}}">{{time}}</time></h5>
								</div>
							</li>
						</ul>
{{/event}}
						<section id="event_details">
							<div id="tabs">
								<ul>
									<li><a href="#attendees">Attendees</a></li>
									<li><a href="#standby">Standby (0)</a></li>
								</ul>
								<div id="attendees">
									<ul>
{{#attendees.active}}
										<li class="{{profession}}">
											<div class="player">
												{{name}}
												<a href="#">?</a>
												<ul>
													<li><div class="comment">{{comment}}</div></li>
												</ul>
											</div>
											<div class="role">{{role}}</div>
										</li>
{{/attendees.active}}
{{^attendees.active}}
										<li>
											No ready attendees yet.
										</li>
{{/attendees.active}}

									</ul>
									{{#signup}}<a href="#">Sign up for event</a>{{/signup}}
									{{#withdraw}}<a class="withdraw" href="{{withdraw}}">Withdraw from event</a>{{/withdraw}}
								</div>
								<div id="standby">
									<ul>
{{#attendees.standby}}
										<li class="{{profession}}">
											<div class="player">
												{{name}}
												<a href="#">?</a>
												<ul>
													<li><div class="comment">{{comment}}</div></li>
												</ul>
											</div>
											<div class="role">{{role}}</div>
										</li>
{{/attendees.standby}}
{{^attendees.standby}}
										<li>
											No one on standby.
										</li>
{{/attendees.standby}}
									</ul>
									{{#signup}}<a href="#">Sign up for event</a>{{/signup}}
									{{#withdraw}}<a class="withdraw" href="{{withdraw}}">Withdraw from event</a>{{/withdraw}}
								</div>
							</div> <!-- end tabs -->
							<div id="event_description">
{{#event}}
								<h6>{{title}}</h6>
								<p>{{description}}</p>
								{{#url}}<a class="build" href="{{url}}" target="_blank" title="Link to more information about {{build}}">{{build}}</a>{{/url}}
								{{^url}}<a class="build_disabled" title="No additional information for {{build}}">{{build}}</a>{{/url}}
{{/event}}
								{{#edit_event}}<a class="edit_event" href="{{edit_event}}">Edit event</a>{{/edit_event}}
								{{#remove_event}}<a class="cancel_event" href="{{remove_event}}">Cancel event</a>{{/remove_event}}
							</div>
						</section><!-- end event details section -->
					</li><!-- end of this event -->
					<div class="event_footer"></div>
